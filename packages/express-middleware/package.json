{
  "name": "@pompelmi/express-middleware",
  "version": "0.14.0-dev.23",
  "description": "Express middleware to scan uploads with pompelmi (YARA).",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**",
    "README.md",
    "LICENSE*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pompelmi/pompelmi.git",
    "directory": "packages/engine"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean --external unzipper,@aws-sdk/client-s3",
    "dev": "tsup src/index.ts --watch --dts --format esm,cjs",
    "test": "vitest run",
    "lint": "echo \"(opzionale)\"",
    "demo": "node tests/demo-server.mjs"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0",
    "pompelmi": "workspace:*"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.10",
    "@vitest/coverage-v8": "2.1.9",
    "express": "^4.19.0",
    "multer": "^2.0.2",
    "pompelmi": "workspace:*",
    "supertest": "^7.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.5.0",
    "vitest": "3.2.4"
  },
  "license": "MIT",
  "dependencies": {
    "file-type": "^18.7.0",
    "unzipper": "^0.12.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.17"
  },
  "private": false
}
