{
  "name": "@pompelmi/ui-react",
  "version": "0.14.0-dev.25",
  "description": "UI components for integrating file upload and malware scanning with pompelmi.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "README.md",
    "LICENSE*"
  ],
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts --format esm,cjs --dts"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "devDependencies": {
    "@types/react": "^19.1.12",
    "@types/react-dom": "^18.2.0",
    "@vitest/coverage-v8": "2.1.9",
    "rimraf": "^5.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0",
    "vitest": "2.1.9"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false
}
