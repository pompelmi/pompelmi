{
  "name": "@pompelmi/ui-react",
  "version": "0.17.0",
  "description": "UI components for integrating file upload and malware scanning with pompelmi.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "README.md",
    "LICENSE*",
    "dist",
    "package.json",
    "CHANGELOG*"
  ],
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts --format esm,cjs --dts",
    "preview": "npm pack --dry-run",
    "prepublishOnly": "npm run build && npx -y publint && npm pack --dry-run",
    "typecheck": "tsc -p tsconfig.json --noEmit || tsc -p tsconfig.build.json --noEmit",
    "typecheck:strict": "tsc -p tsconfig.strict.json --noEmit"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "devDependencies": {
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitest/coverage-v8": "2.1.9",
    "rimraf": "^5.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "2.1.9"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "typings": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/pompelmi/pompelmi",
    "directory": "packages/ui-react"
  }
}
