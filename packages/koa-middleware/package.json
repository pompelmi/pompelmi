{
  "name": "@pompelmi/koa-middleware",
  "version": "0.16.0-dev.41",
  "description": "Koa middleware to scan uploads with pompelmi (YARA), MIME sniffing and ZIP anti-bomb.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**",
    "README.md",
    "LICENSE*",
    "dist",
    "package.json",
    "CHANGELOG*"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup --clean --dts --format esm,cjs",
    "dev": "tsup --watch --dts --format esm,cjs",
    "demo": "node tests/demo-server.mjs",
    "preview": "npm pack --dry-run",
    "prepublishOnly": "npm run build && npx -y publint && npm pack --dry-run",
    "typecheck": "tsc -p tsconfig.json --noEmit || tsc -p tsconfig.build.json --noEmit",
    "typecheck:strict": "tsc -p tsconfig.strict.json --noEmit"
  },
  "peerDependencies": {
    "@koa/multer": "^4.0.0",
    "@koa/router": "^12.0.0",
    "koa": "^2.14.0 || ^3.0.0",
    "pompelmi": "workspace:*",
    "multer": "^2.0.2"
  },
  "dependencies": {},
  "devDependencies": {
    "@koa/multer": "^4.0.0",
    "@koa/router": "^12.0.2",
    "@types/node": "^20.19.10",
    "@vitest/coverage-v8": "2.1.9",
    "koa": "^2.16.1",
    "pompelmi": "workspace:*",
    "tsup": "^8.5.0",
    "typescript": "^5.5.0",
    "vitest": "2.1.9",
    "@types/koa": "^2.13.9",
    "multer": "^2.0.2"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "engines": {
    "node": ">=18.17"
  },
  "private": false,
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/pompelmi/pompelmi",
    "directory": "packages/koa-middleware"
  }
}
