{
  "name": "@pompelmi/fastify-plugin",
  "version": "0.13.2",
  "description": "Fastify upload scanning plugin for pompelmi (YARA) with MIME sniffing and ZIP limits.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/pompelmi/pompelmi.git",
    "directory": "packages/engine"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean --external unzipper,@aws-sdk/client-s3",
    "dev": "tsup src/index.ts --watch --dts --format esm,cjs",
    "demo": "node packages/fastify-plugin/tests/demo-server.mjs"
  },
  "peerDependencies": {
    "@fastify/multipart": "^7.0.0",
    "fastify": "^4.0.0",
    "pompelmi": "workspace:*"
  },
  "devDependencies": {
    "@fastify/multipart": "^9.0.3",
    "@types/node": "^20.19.10",
    "@vitest/coverage-v8": "2.1.9",
    "fastify": "^4.28.0",
    "file-type": "^18.7.0",
    "tsup": "^8.5.0",
    "typescript": "^5.5.0",
    "unzipper": "^0.12.3",
    "vitest": "2.1.9"
  },
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.17"
  }
}
