{
  "name": "@pompelmi/engine-ghidra",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "preview": "npm pack --dry-run",
    "prepublishOnly": "npm run build && npx -y publint && npm pack --dry-run"
  },
  "dependencies": {
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/tmp": "^0.2.6",
    "@vitest/coverage-v8": "2.1.9",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0",
    "vitest": "2.1.9"
  },
  "peerDependencies": {
    "pompelmi": "workspace:*"
  },
  "files": [
    "dist/**",
    "README.md",
    "LICENSE*",
    "dist",
    "package.json",
    "CHANGELOG*",
    "scripts/**/*.java"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/pompelmi/pompelmi",
    "directory": "packages/engine-ghidra"
  },
  "homepage": "https://pompelmi.github.io/pompelmi/",
  "description": "Ghidra P-Code decompilation engine for Pompelmi file scanner",
  "keywords": [
    "security",
    "malware",
    "scanner",
    "ghidra",
    "pcode",
    "decompilation",
    "reverse-engineering",
    "static-analysis"
  ]
}