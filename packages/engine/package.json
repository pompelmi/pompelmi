{
  "name": "@pompelmi/engine",
  "version": "0.16.0-dev.6",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pompelmi/pompelmi.git",
    "directory": "packages/engine"
  },
  "files": [
    "dist/**",
    "README.md",
    "LICENSE*",
    "dist",
    "package.json",
    "CHANGELOG*"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "dev",
    "provenance": true
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.cjs",
    "schema:json": "node --input-type=module -e \"import('./dist/index.mjs').then(m=>console.log(JSON.stringify(m.resultJsonSchema, null, 2)))\" > result.schema.json",
    "test": "vitest run",
    "preview": "npm pack --dry-run",
    "prepublishOnly": "npm run build && npx -y publint && npm pack --dry-run",
    "typecheck": "tsc -p tsconfig.json --noEmit || tsc -p tsconfig.build.json --noEmit",
    "typecheck:strict": "tsc -p tsconfig.strict.json --noEmit"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.0",
    "@fastify/multipart": "^9.2.1",
    "fastify": "^5.6.1",
    "yauzl": "^3.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "@types/yauzl": "^2.10.3",
    "@vitest/coverage-v8": "2.1.9",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "1.6.0",
    "yazl": "^3.3.1",
    "zod-to-json-schema": "^3.23.5"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "typings": "dist/index.d.ts"
}
